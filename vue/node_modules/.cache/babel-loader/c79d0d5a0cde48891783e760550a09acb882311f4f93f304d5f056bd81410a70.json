s: {\r\n            pageNum: this.pageNum,\r\n            pageSize: this.pageSize,\r\n            name: this.name,\r\n          }\r\n        }).then(res => {\r\n          if(res.code === '200'){\r\n            console.log(res);\r\n          this.goodsList = res.data?.list\r\n          this.total = res.data?.total\r\n          }\r\n           else {\r\n          this.$message.error(res.msg)\r\n           }\r\n        })\r\n      },\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.bg{\r\n  height: 400px;\r\n  background-image: url(\"@/assets/imgs/home.png\");\r\n  background-size: 100%;\r\n  background-position-y: center;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n</style>"],"mappings":"AAyCA;EAEAA,KAAA;IACA;MACAC,YAAA;MACAC,SAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,QAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;IACA,KAAAC,SAAA;EACA;EACA;EACAC,OAAA;IACAF,aAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,wBAAAC,IAAA,CAAAC,GAAA;QACA,KAAAf,YAAA,GAAAe,GAAA,CAAAhB,IAAA;MACA;IACA;IACAW,UAAAP,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAS,QAAA,CAAAC,GAAA;QACAG,MAAA;UACAb,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAa,IAAA,OAAAA;QACA;MACA,GAAAH,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAG,IAAA;UACAC,OAAA,CAAAC,GAAA,CAAAL,GAAA;UACA,KAAAd,SAAA,GAAAc,GAAA,CAAAhB,IAAA,EAAAsB,IAAA;UACA,KAAAhB,KAAA,GAAAU,GAAA,CAAAhB,IAAA,EAAAM,KAAA;QACA,OACA;UACA,KAAAiB,QAAA,CAAAC,KAAA,CAAAR,GAAA,CAAAS,GAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}